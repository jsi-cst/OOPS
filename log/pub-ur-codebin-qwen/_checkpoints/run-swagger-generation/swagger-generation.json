[
    [
        {
            "method": "POST",
            "apiurl": "/api/paste",
            "feat": "/api/paste"
        },
        {
            "req": [
                {
                    "in": "body",
                    "name": "body",
                    "description": "Request body for creating a new code paste",
                    "required": true,
                    "schema": {
                        "type": "object",
                        "properties": {
                            "paste_title": {
                                "type": "string",
                                "description": "Title of the paste",
                                "default": "Untitled Paste",
                                "minLength": 1,
                                "maxLength": 50
                            },
                            "paste_syntax": {
                                "type": "string",
                                "description": "Syntax highlighting for the paste",
                                "enum": [
                                    "NONE",
                                    "JAVA",
                                    "CSHARP",
                                    "Python",
                                    "JAVASCRIPT",
                                    "GO",
                                    "CLANG",
                                    "CPLUSPLUS",
                                    "PHP",
                                    "SWIFT",
                                    "LUA",
                                    "RUBY",
                                    "MYSQL",
                                    "POSTGRESQL"
                                ],
                                "default": "NONE"
                            },
                            "paste_visibility": {
                                "type": "string",
                                "description": "Visibility of the paste",
                                "enum": [
                                    "PUBLIC",
                                    "PRIVATE"
                                ],
                                "default": "PRIVATE"
                            },
                            "paste_expiration": {
                                "type": "string",
                                "description": "Expiration time for the paste",
                                "enum": [
                                    "TENMINUTES",
                                    "THIRTYMINUTES",
                                    "ONEHOUR",
                                    "FIVEHOURS",
                                    "ONEDAY",
                                    "THREEDAYS",
                                    "FIVEDAYS",
                                    "ONEWEEK"
                                ],
                                "default": "ONEHOUR"
                            },
                            "source_code": {
                                "type": "string",
                                "description": "The actual source code content of the paste - required field"
                            }
                        },
                        "required": [
                            "source_code"
                        ]
                    }
                }
            ],
            "res": {
                "200": {
                    "description": "Successfully created a new code paste",
                    "schema": {
                        "type": "object",
                        "properties": {
                            "paste_id": {
                                "type": "string",
                                "format": "uuid",
                                "description": "Unique identifier of the paste"
                            },
                            "source_code": {
                                "type": "string",
                                "description": "The source code content of the paste"
                            },
                            "paste_title": {
                                "type": "string",
                                "description": "Title of the paste"
                            },
                            "paste_syntax": {
                                "type": "string",
                                "enum": [
                                    "NONE",
                                    "JAVA",
                                    "PYTHON",
                                    "JAVASCRIPT",
                                    "HTML",
                                    "CSS",
                                    "C",
                                    "CPP",
                                    "CSHARP",
                                    "PHP",
                                    "RUBY",
                                    "GO",
                                    "SWIFT",
                                    "KOTLIN",
                                    "TYPESCRIPT",
                                    "RUST",
                                    "SCALA",
                                    "PERL",
                                    "SHELL",
                                    "SQL",
                                    "XML",
                                    "JSON"
                                ],
                                "description": "Syntax highlighting for the paste"
                            },
                            "paste_expiration_date": {
                                "type": "string",
                                "format": "date-time",
                                "pattern": "yyyy-MM-dd HH:mm:ss",
                                "description": "Expiration date of the paste"
                            },
                            "paste_visibility": {
                                "type": "string",
                                "enum": [
                                    "PUBLIC",
                                    "PRIVATE"
                                ],
                                "description": "Visibility setting of the paste"
                            }
                        },
                        "required": [
                            "paste_id",
                            "source_code",
                            "paste_title",
                            "paste_syntax",
                            "paste_expiration_date",
                            "paste_visibility"
                        ]
                    }
                },
                "400": {
                    "description": "Bad request - invalid input parameters",
                    "schema": {
                        "type": "object",
                        "properties": {
                            "timestamp": {
                                "type": "string",
                                "format": "date-time",
                                "pattern": "yyyy-MM-dd HH:mm:ss",
                                "description": "Timestamp of the error"
                            },
                            "status": {
                                "type": "integer",
                                "description": "HTTP status code"
                            },
                            "error": {
                                "type": "string",
                                "description": "Error reason phrase"
                            },
                            "message": {
                                "type": "string",
                                "description": "Error message"
                            }
                        },
                        "required": [
                            "timestamp",
                            "status",
                            "error",
                            "message"
                        ]
                    }
                }
            }
        }
